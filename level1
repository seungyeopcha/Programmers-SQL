-- 모든 레코드 조회하기
SELECT *
from animal_ins
order by animal_id;

-- 최댓값 구하기
SELECT datetime
from animal_ins
order by datetime desc
limit 1;

-- 이름이 없는 동물의 아이디
SELECT animal_id
from animal_ins
where name is null
order by animal_id;

-- 역순 정렬하기
SELECT name, datetime
from animal_ins
order by animal_id desc;
